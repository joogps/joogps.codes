{
    "version": 2,
    "alias": "joogps.com",
    "routes": [
        {
        "src": "/push/(.*)",
        "dest": "/push/$1"
        }
    ],
    "headers": [
        {
        "source": "/(.*)",
        "headers": [
            {
            "key": "Cache-Control",
            "value": "no-cache"
            }
        ]
        }
    ]
}
  